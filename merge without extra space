class Solution {

    public void merge(int arr1[], int arr2[], int n, int m) {
       
       for(int i=0;i<n;i++){
       if(arr1[i] > arr2[0]){
           int temp = arr1[i];
           arr1[i] = arr[j];
           arr[j] =temp;
            
            
           int first = arr2[0];
           for(int j=1;j<m && arr2[j] < first;j++){
                arr[j-1] = first;
           }
           arr[j-1] =first;
       }
